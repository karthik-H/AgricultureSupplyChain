<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
<class name="Esp8266.Wifi.farmer.Farmer" table="farmer">
<id name="id" column="id" type="int" >
<generator class="increment"/>
</id>
<property name="farmer_first_name" column="farmer_first_name" type="string"/>
<property name="farmer_last_name" column="farmer_last_name" type="string"/>
<property name="username" column="username" type="string"/>
<property name="password" column="password" type="string"/>
<property name="farmer_address" column="farmer_address" type="string"/>
<property name="water_availablity" column="water_availablity" type="float"/>
<property name="electricity_availablity" column="electricity_availablity" type="float"/>




</class>

</hibernate-mapping>