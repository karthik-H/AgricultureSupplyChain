<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 


<hibernate-mapping>
<class name="Esp8266.Wifi.crop.Crop" table="crop_data">
<id name="id" column="id" type="int" >
<generator class="native"/>
</id>
<property name="crop_name" column="crop_name" type="string"/>
<property name="crop_available" column="crop_available" type="int"/>
<property name="cost" column="cost" type="float"/>
</class>
 
</hibernate-mapping>