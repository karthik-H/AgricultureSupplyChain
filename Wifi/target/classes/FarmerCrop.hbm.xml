<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 


<hibernate-mapping>
<class name="Esp8266.Wifi.cropavailable.FarmerCrop" table="farmer_crop">

<id name="id" column="id" type="int" >
<generator class="increment"/>
</id>
<property name="farmer_id" column="farmer_id" type="int" />
<property name="crop_id" column="crop_id" type="int"/>
<property name="date" column="date" type="date"/>
<property name="iteration" column="iteration" type="int"/>
<property name="quality" column="quality" type="int"/>
</class>
 
</hibernate-mapping>