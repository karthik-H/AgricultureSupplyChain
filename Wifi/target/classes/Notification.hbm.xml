<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 


<hibernate-mapping>
<class name="Esp8266.Wifi.notification.Notification" table="notification">

<id name="id" column="id" type="int" >
<generator class="increment"/>
</id>
<property name="notification_topic" column="notification_topic" type="string"/>
<property name="notification_message" column="notification_message" type="string"/>

</class>
 
</hibernate-mapping>